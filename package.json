{
  "name": "unconf2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4250",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:status": "dotenv -e .env.local -- prisma migrate status",
    "db:migrate:resolve": "dotenv -e .env.local -- prisma migrate resolve",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:setup": "npm run db:migrate && npm run db:generate && npm run db:seed",
    "db:verify": "npm run db:migrate:status && npm run db:generate && npm run type-check",
    "db:ci": "scripts/migrate.sh",
    "db:ci:dry-run": "DRY_RUN=true scripts/migrate.sh",
    "db:prod:migrate": "NODE_ENV=production scripts/migrate.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@supabase/supabase-js": "^2.50.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.513.0",
    "next": "^14.2.18",
    "next-themes": "^0.4.6",
    "prisma": "^6.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8.5.4",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
